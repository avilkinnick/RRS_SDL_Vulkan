<?xml version="1.0" encoding="UTF-8"?>
<Config>
	<Device>
		<Order>1</Order>
		<SubStepNum>1</SubStepNum>
		<Solver>euler</Solver>

		<!-- Начальное состояние концевого крана: 0 - закрыт, 1 - открыт -->
		<IsOpened>1</IsOpened>

		<!-- Время переключения концевого крана, с -->
		<SwitchTime>0.2</SwitchTime>

		<!-- Коэффициент перетока из рукава в магистраль через открытый концевой кран -->
		<kPipe>0.5</kPipe>

		<!-- Коэффициент разрядки из рукава в атмосферу при закрытом концевом кране -->
		<kAtm>0.1</kAtm>

		<!-- Коэффициент громкости озвучки к потоку опорожнения рукава через атмосферное отверстие -->
		<K_sound>2.5</K_sound>

		<!-- Смещение точки крепления рукава от продольной оси (<0 - влево, >0 - вправо) -->
		<ShiftSide>0.3</ShiftSide>

		<!-- Смещение точки крепления рукава по продольной оси от оси автосцепки -->
		<ShiftCoord>0.4</ShiftCoord>
	</Device>
</Config>