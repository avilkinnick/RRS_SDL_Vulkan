<?xml version="1.0" encoding="UTF-8"?>
<Config>
    <Device>
        <Order>1</Order>
        <SubStepNum>1</SubStepNum>
        <Solver>euler</Solver>

        <!-- Темп наполнения рабочей камеры из ЗР -->
        <K1>2.0e-4</K1>

        <!-- Темп опорожнения рабочей камеры  -->
        <K2>1.5e-4</K2>

        <!-- Темп наполнения ТЦ через реле давления-->
        <K3>5.0e-2</K3>

        <!-- Темп опорожнения ТЦ через реле давления -->
        <K4>5.0e-2</K4>

        <!-- Коэффициент чувствительности к разнице давлений в рабочей камере и в ЗР/атмосферного -->
        <A1>1.0e-3</A1>
        <!-- Коэффициент чувствительности к клапану наполнения/опорожнения ТЦ в реле давления -->
        <A2>5.0</A2>

        <!-- Объем рабочей камеры -->
        <V0>1.5e-3</V0>

        <!-- Сопротивление катушек вентилей, Ом -->
        <R>400.0</R>
        <!-- Индуктивность катушек вентилей, Гн -->
        <L>0.5</L>
        <!-- Ток включения вентилей, А -->
        <I_on>0.1</I_on>

    </Device>
</Config>