<?xml version="1.0" encoding="UTF-8"?>
<Config>
	<Joint>
		<ModuleName>joint-coupling-sa3</ModuleName>

		<!-- Начальное состояние сцепки: 0 - расцеплены, 1 - сцеплены -->
		<initConnectionState>0</initConnectionState>

		<!-- Зазор в сцепном устройстве -->
		<delta>0.011</delta>
		<!-- Упругость поглощающих аппаратов -->
		<c>2e7</c>
		<!-- Коэффициент трения поглощающих аппаратов -->
		<f>0.6</f>
		<!-- Максимальное сжатие поглощающих аппаратов -->
		<lambda>0.090</lambda>
		<!-- Упругость конструкций после сжатия поглощающих аппаратов -->
		<ck>2e8</ck>
		<!-- Коэффициент потерь энергии на пластические деформации конструкций -->
		<fk>0.5</fk>
		<!-- Усилие до начала сжатия поглощающего аппарата -->
		<T0>5e3</T0>
	</Joint>
</Config>